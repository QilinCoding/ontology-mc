     xml:base="http://www.nmdc.com/ontologies/mc"
     xmlns:mc="http://www.nmdc.com/ontologies/mc/1.0.0#"
     xmlns:mc1="http://www.nmdc.com/ontologies/mc#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:untitled-ontology-6="http://www.semanticweb.org/wsz/ontologies/2023/9/untitled-ontology-6#">
    <owl:Ontology rdf:about="http://www.nmdc.com/ontologies/mc">
        <owl:versionIRI rdf:resource="http://www.nmdc.com/ontologies/mc/1.0.0"/>
        <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    </owl:Ontology>
    


